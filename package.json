{
  "name": "filevis",
  "version": "1.0.0",
  "description": "An application for visualizing summary information for files in a specified directory ",
  "main": "index.ts",
  "scripts": {
    "test": "(cd src && jest)",
    "build": "./node_modules/typescript/bin/tsc -p src",
    "webpack": "./node_modules/.bin/webpack",
    "start": "(npm run build && npm run webpack && node dist/index.js)"
  },
  "author": "Adam Beaton",
  "license": "SEE LICENSE IN LICENSE.txt",
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.91",
    "@types/promise-polyfill": "^6.0.0",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "express": "^4.16.2",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "promise-polyfill": "^6.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-vis": "^1.8.1",
    "styled-components": "^2.4.0",
    "styled-components-ts": "0.0.7",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "@types/node": "^8.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
